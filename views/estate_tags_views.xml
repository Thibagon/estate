<?xml version="1.0"?>
    <odoo>
        <record id="estate_tags_model_action" model="ir.actions.act_window">
            <field name="name">Tags</field>
            <field name="res_model">estate_tags</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="estate_tags_tree_view" model="ir.ui.view">
            <field name="name">Tags list</field>
            <field name="model">estate_tags</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>
    </odoo>